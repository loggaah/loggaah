{
  "extensions": {
    "processors": [ "path/to/processor.js", "processor-module-name" ],
    "appenders": [ "path/to/appender.js", "appender-module-name" ],
    "configurators": [ "path/to.configurator.js", "configurator-module-name" ]
  },
  "configurators": {
    "JsonConfigurator": {
      "rescan": 30
    },
    "RestConfigurator": {
      "port": 8080,
      "bind": "0.0.0.0",
      "password": "notverysafe",
      "ssl": false
    }
  },
  "processors": {
      "formatter": {
        "format": "%m%n"
      }
  },
  "appenders": {
    "STDOUT": {
      "type": "console",
      "processor": "formatter",
      "color": false
    },
    "appLog": {
      "type": "file",
      "processor": [ "formatter" ],
      "rollover": {
        "size": "100MB",
        "pattern": "yyyy-MM-dd"
      }
    }
  },
  "loggers": {
    "testLogger": {
      "level": "DEBUG",
      "match": "lib/.*",
      "appender": [ "STDOUT" ]
    }
  },
  "root": {
    "level": "info",
    "appender": "STDOUT"
  }
}